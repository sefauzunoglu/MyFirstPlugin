<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.bridge.myfirstplugin" version="0.0.1"
	xmlns="http://apache.org/cordova/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<name>MyFirstPlugin</name>
	<js-module name="MyFirstPlugin" src="www/MyFirstPlugin.js">
		<clobbers target="cordova.plugins.MyFirstPlugin" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="MyFirstPlugin">
				<param name="android-package" value="com.bridge.myfirstplugin.MyFirstPlugin" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml"></config-file>
		<source-file src="src/android/MyFirstPlugin.java" target-dir="src/com/bridge/myfirstplugin/MyFirstPlugin" />
	</platform>
</plugin>